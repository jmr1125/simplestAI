#include <cstdio>
#include <stdio.h>
using namespace std;
int main(int argc, char *argv[]) {
  if (argc != 2) {
    fprintf(stderr, "usage: %s <file>", argv[0]);
    return 1;
  }
  int32_t magicnumber;
  FILE *fp = fopen(argv[1], "rb");
  fread(&magicnumber,1,sizeof magicnumber,fp);
  printf("magicnumber: %x",magicnumber);
  fclose(fp);
}
